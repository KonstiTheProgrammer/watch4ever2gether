{
  "manifest_version": 3,
  "name": "Meine P2P-Erweiterung",
  "version": "1.0.0",
  "description": "Eine Chrome-Erweiterung, die P2P-Kommunikation erm√∂glicht.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
